{
  "name": "@kmcbride3/n8n-nodes-discord",
  "version": "0.6",
  "description": "nodes to trigger workflows from Discord or send interactive messages. Uses the components API which allows to create dialogs (e.g. attach buttons and wait for the user to click on them)",
  "keywords": [
    "n8n",
    "node",
    "discord",
    "trigger",
    "n8n-node",
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kmcbride3/n8n-nodes-discord",
  "author": {
    "name": "kmcbride3",
    "url": "https://github.com/kmcbride3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmcbride3/n8n-nodes-discord.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp",
    "dev": "tsc --watch",
    "lint:fix": "eslint ./src -c eslint.config.mjs --quiet --fix",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{ts,tsx,css,md,json}\" ",
    "lint": "pnpm lint:format && pnpm lint:fix ",
    "publish": "pnpm build && pnpm publish --access public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DiscordApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Discord/Discord.node.js",
      "dist/nodes/Discord/DiscordTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/express": "^5.0.0",
    "@types/request-promise-native": "~1.0.21",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "gulp": "^5.0.0",
    "n8n-core": "^1.63.4",
    "n8n-workflow": "^1.63.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "@discordjs/rest": "^2.4.0",
    "@eslint/compat": "^1.2.1",
    "@types/node-ipc": "^9.2.3",
    "axios": "^1.7.7",
    "discord-api-types": "^0.37.103",
    "discord.js": "^14.16.3",
    "node-ipc": "^12.0.0",
    "prettier-eslint": "^16.3.0",
    "uid": "^2.0.2"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
}
