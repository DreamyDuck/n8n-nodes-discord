{
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2019",
    "lib": ["es2019", "es2020", "es2022.error"],
    "removeComments": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "module": "ESNext",
    "skipLibCheck": true,
    "rootDir": "src",
    //"emitDeclarationOnly": true,
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*"]
}
